<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:id="@+id/testsublayout" >
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:src="@drawable/title" />
    
    <TextView
        android:id="@+id/testsubtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="128px"
        android:layout_marginTop="20px"
        android:typeface="sans"
        android:textSize="28px"
        android:textColor="#565656"
        android:shadowColor="#7F000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1.2" />
    
    <ImageView
        android:id="@+id/device" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="520px"
        android:layout_marginTop="12px" />
    
   	<TextView
        android:id="@+id/timeText" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:typeface="sans"
        android:textSize="24px"
        android:textColor="#444444"
        android:shadowColor="#7F000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1.2"
        android:layout_marginLeft="578px"
        android:layout_marginTop="15px" />
   	
   	<Button
        android:id="@+id/escicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="728px"
        android:background="@drawable/esc_selector" />
   	
   	<TextView
   	    android:id="@+id/testsubstate"
        android:layout_width="391px"
        android:layout_height="wrap_content"
        android:typeface="sans"
        android:textSize="24px"
        android:layout_marginLeft="200px"
        android:layout_marginTop="120px"
        android:gravity="center" />
   	
    <TextView
        android:id="@+id/text1title"
        android:layout_width="100px"
        android:layout_height="62px"
        android:layout_marginLeft="200px"
        android:layout_marginTop="170px"
        android:paddingLeft="10px"
        android:textSize="18px"
        android:textColor="#565656"
        android:gravity="center" />
    
    <TextView
        android:id="@+id/text1"
        android:layout_width="271px"
        android:layout_height="62px"
        android:layout_marginLeft="320px"
        android:layout_marginTop="170px"
        android:paddingLeft="10px"
        android:textSize="24px"
        android:textColor="#565656"
        android:gravity="center"
        android:background="@drawable/af_intercept_box" />
    
    <TextView
        android:id="@+id/text2title"
        android:layout_width="100px"
        android:layout_height="62px"
        android:layout_marginLeft="200px"
        android:layout_marginTop="260px"
        android:paddingLeft="10px"
        android:textSize="18px"
        android:textColor="#565656"
        android:gravity="center" />
    
    <TextView
        android:id="@+id/text2"
        android:layout_width="271px"
        android:layout_height="62px"
        android:layout_marginLeft="320px"
        android:layout_marginTop="260px"
        android:paddingLeft="10px"
        android:textSize="24px"
        android:textColor="#565656"
        android:gravity="center"
        android:background="@drawable/af_intercept_box" />
    
    <Button
        android:id="@+id/runbtn"
        android:layout_width="129px"
        android:layout_height="113px"
        android:layout_marginLeft="13px"
        android:layout_marginTop="322px"
        android:gravity="center"
	    android:text="RUN"
	    android:textStyle="bold"
        android:typeface="sans"
        android:textSize="28px"
        android:textColor="#FFFFFF"
        android:background="@drawable/quick_start_selector" />
    
    <Button
        android:id="@+id/cancelbtn"
        android:layout_width="129px"
        android:layout_height="114px"
        android:layout_marginLeft="660px"
        android:layout_marginTop="322px"
        android:gravity="center"
	    android:text="CANCEL"
	    android:textStyle="bold"
        android:typeface="sans"
        android:textSize="28px"
        android:textColor="#FFFFFF"
        android:background="@drawable/full_start_selector" />

</RelativeLayout>